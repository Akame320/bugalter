mixin advantagesCol(object)
    if(object.activeMobile)
        .advantages-section__col.active-tablet
            .advantages-section__col-inner
                .advantages-section__col-img
                    +svgIcon({name: object.icon})
                .advantages-section__col-text !{object.text}
    else
        .advantages-section__col
            .advantages-section__col-inner
                .advantages-section__col-img
                    +svgIcon({name: object.icon})
                .advantages-section__col-text !{object.text}

mixin calculateServicesInput(object)
    if(object.tabIndex)
        .calculate-section__services-input
            if(object.checked)
                input(type="radio" name=object.name id=object.id value=object.value checked)
            else
                input(type="radio" name=object.name id=object.id value=object.value )
            label.calculate-section__services-tab.radio-label(for=object.id data-tab=object.tabIndex) !{object.text}
    else
        .calculate-section__services-input
            if(object.checked)
                input(type="radio" name=object.name id=object.id value=object.value checked)
            else
                input(type="radio" name=object.name id=object.id value=object.value )
            label.calculate-section__services-tab.radio-label(for=object.id) !{object.text}

mixin calculateCartItem(object)
    .calculate-section__cart-item
        .calculate-section__cart-inner(style='background-image: url("images/content/calculate-section/bg.jpg")')
            .calculate-section__cart-text !{object.text}
            .calculate-section__cart-input
                if(object.checked)
                    input(type="checkbox" name=object.name id=object.id value=object.text checked)
                else
                    input(type="checkbox" name=object.name id=object.id value=object.value )
                label(for=object.id).cart-label
                    span.block
                    span Выбрать

mixin slide(object)
    .reviews-section__slide
        a(href=object.img + '-pc.jpg').reviews-section__slide-inner
            picture
                source(srcset=object.img + '-pc.webp' media="(min-width: 767px)" alt=object.alt type='image/webp')
                source(srcset=object.img + '-mb.webp' media="(min-width: 0) and (max-width: 767px)" alt=object.alt type='image/webp')
                img(src=object.img + '-pc.png' srcset=object.img + '-mb.png' + ' 767w' alt=object.alt)
mixin qustionsRow(object)
